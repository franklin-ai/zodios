{
  "name": "@zodios/core",
  "description": "Typescript API client with autocompletion and zod validations",
  "version": "12.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/*.types": {
      "import": "./dist/*.types.mjs",
      "require": "./dist/*.types.js",
      "types": "./dist/*.types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "ecyrbe",
    "email": "ecyrbe@gmail.com"
  },
  "contributors": [
    {
      "name": "chrisbenincasa"
    },
    {
      "name": "nmcdaines"
    }
  ],
  "homepage": "https://github.com/ecyrbe/zodios",
  "repository": {
    "type": "git",
    "url": "https://github.com/ecyrbe/zodios.git"
  },
  "license": "MIT",
  "keywords": [
    "axios",
    "openapi",
    "zod",
    "autocomplete",
    "validation"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "example": "ts-node examples/jsonplaceholder.ts",
    "example:dev.to": "ts-node examples/dev.to/example.ts",
    "major-rc": "npm version premajor --preid=rc",
    "minor-rc": "npm version preminor --preid=rc",
    "patch-rc": "npm version prepatch --preid=rc",
    "rc": "npm version prerelease --preid=rc",
    "build": "tsup",
    "test": "jest --coverage"
  },
  "peerDependencies": {
    "axios": "^0.x || ^1.0.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@jest/types": "29.6.3",
    "@types/express": "4.17.19",
    "@types/jest": "29.5.5",
    "@types/multer": "1.4.8",
    "@types/node": "20.8.9",
    "axios": "1.11.0",
    "express": "4.21.2",
    "form-data": "4.0.4",
    "jest": "29.7.0",
    "multer": "1.4.5-lts.1",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsup": "^8.5.0",
    "typescript": "5.8.3",
    "zod": "^4.0.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
